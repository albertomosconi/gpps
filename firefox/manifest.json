{
  "name": "GPPS",
  "description": "Google Privacy Page Skip",
  "manifest_version": 2,
  "version": "1.1",
  "icons": {
    "16": "/images/16.png",
    "32": "/images/32.png",
    "48": "/images/48.png",
    "128": "/images/128.png"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "{f40b6e2f-8e25-4ed7-8bec-50c360547a1f}",
      "strict_min_version": "57.0"
    }
  },
  "permissions": ["storage", "activeTab"],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": "/images/48.png",
    "default_title": "YT Skip",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["https://consent.youtube.com/*", "https://*.google.com/*"],
      "js": ["./js/skip.js"]
    }
  ]
}
